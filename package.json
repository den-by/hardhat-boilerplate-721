{
  "author": "Dzianis Buyakou",
  "name": "hardhat-boilerplate-721",
  "version": "1.0.0",
  "description": "A boilerplate repository to get you started with Hardhat",
  "scripts": {
    "compile": "hardhat compile",
    "deploy": "hardhat run --network mumbai ./scripts/deploy.js",
    "mint": "hardhat run --network mumbai ./scripts/mint.js",
    "node": "hardhat node",
    "format": "prettier --check \"**/*.{ts,js,sol}\"",
    "format:fix": "prettier --write \"**/*.{ts,js,sol}\""
  },
  "license": "MIT",
  "keywords": [
    "hardhat",
    "boilerplate",
    "ethereum",
    "solidity",
    "starter",
    "ERC721",
    "mumbai"
  ],
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "hardhat": "^2.13.0",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.8.2",
    "dotenv": "^16.0.3"
  }
}
